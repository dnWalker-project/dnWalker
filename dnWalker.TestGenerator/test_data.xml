<?xml version="1.0" encoding="utf-8" ?>
<TestData>
    <MethodName Kind="static" Name="SingleBranching" TypeName="Examples.Concolic.Simple.Branches">
        <TestCase IsFaulted="false">
            <Arguments>
                <Int32Parameter Name="x" Value="-5"/>
            </Arguments>
            <Results>
                <Int32Parameter Name="#__RETURN_VALUE__" Value="3"/>
            </Results>
        </TestCase>
        <TestCase IsFaulted="true">
            <Arguments>
                <Int32Parameter Name="x" Value="0"/>
            </Arguments>
        </TestCase>
    </MethodName>

    <MethodName Kind="instance" Name="SingleBranchingInstance" TypeName="Examples.Concolic.Simple.Branches">
        <TestCase IsFaulted="false">
            <Arguments>
                <ObjectParameter Type="Examples.Concolic.Simple.Branches" IsThisArgument="true" IsNull="false">
                    <Field Name="_id">
                        <Int32Parameter Value="2"/>
                    </Field>
                </ObjectParameter>
                <Int32Parameter Name="x" Value="-5"/>
            </Arguments>
            <Results>
                <Int32Parameter Name="#__RETURN_VALUE__" Value="3"/>
            </Results>
        </TestCase>
    </MethodName>
</TestData>